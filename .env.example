# Backend Environment Variables (backend/.env)
# MongoDB - Use your MongoDB Atlas connection string
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/ridesharing

# JWT Secret
JWT_SECRET=your_jwt_secret_key_here

# Server Port
PORT=5000

# Frontend Environment Variables (root .env)
# API URL pointing to your Render backend - used in all environments
# Frontend connects directly to Render (no localhost proxy)
VITE_API_URL=https://rideshareproject-vyu1.onrender.com/api